{
  "expo": {
    "name": "little-lemon-app",
    "slug": "little-lemon-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/little-lemon-logo-grey.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "fonts": [
      {
        "asset": "assets/fonts/Karla-Regular.ttf",
        "font-style": "normal"
      },
      {
        "asset": "assets/fonts/MarkaziText-Regular.ttf",
        "font-style": "normal"
      }
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
